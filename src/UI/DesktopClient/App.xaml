<Application x:Class="UexCorpDataRunner.DesktopClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/StyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="Nanito">./Fonts/#Nunito</FontFamily>
            <Style TargetType="TextBlock">
                <Setter Property="FontFamily" Value="{StaticResource Nanito}" />
            </Style>
            <Style TargetType="TextBox">
                <Setter Property="FontFamily" Value="{StaticResource Nanito}" />
            </Style>
            <!--<BooleanToVisibilityConverter x:Key="boolToVisibilityConverter" />-->
        </ResourceDictionary>
    </Application.Resources>
</Application>
