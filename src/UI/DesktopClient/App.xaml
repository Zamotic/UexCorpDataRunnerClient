<Application x:Class="UexCorpDataRunner.DesktopClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UexCorpDataRunner.DesktopClient">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <local:SkinResourceDictionary 
                    DarkSource="pack://application:,,,/Presentation;component/Styles/DarkTheme.xaml" 
                    LightSource="pack://application:,,,/Presentation;component/Styles/LightTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Styles/StyleDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
